<%= simple_form_for(@stack_word_tree) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :data_file %>
    </div>

    <%= render partial: "results", locals: { stack_word_tree: @stack_word_tree }  %>
    <div class="form-actions">
      <%= f.button :submit, class: "btn btn-success" %>
    </div>
<% end %>
